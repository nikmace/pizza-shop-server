{
  "version": 1,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "MONGO_USER": "@snap_mongo_user",
    "MONGO_PW": "@snap_mongo_pw",
    "PORT": "@snap_port"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET"]
    }
  ]
}
